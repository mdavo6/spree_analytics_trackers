<% if tracker = Spree::Tracker.current(:google_analytics) %>
  <script>
    if (typeof ga !== 'undefined') {
      <% products.each_with_index do |product, index| %>
        ga('ec:addImpression',
          <%= product_for_google(product, position: index+1) %>
        );
      <% end %>
    }
  </script>
<% end %>
